{
    "name": "ZendGeoip",
    "description": "MaxMind GeoIP Module for Zend Framework 3",
    "license": "BSD-3-Clause",
    "keywords": [
        "geoip",
        "zf2",
        "maxmind"
    ],
    "authors": [
        {
            "name": "Witold Wasiczko",
            "email": "witold@wasiczko.pl",
            "homepage": "http://www.psd2html.pl/"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "geoip/geoip": "^1.17",
        "guzzlehttp/guzzle": "~5.0",
        "zendframework/zend-mvc": "^3.0",
        "zendframework/zend-modulemanager": "~2.0 || ~3.0",
        "zendframework/zend-servicemanager": "^3.3",
        "zendframework/zend-eventmanager": "^3.1",
        "zendframework/zend-console": "^2.6",
        "zendframework/zend-view": "^2.8",
        "zendframework/zend-hydrator": "^2.2",
        "zendframework/zend-filter": "^2.7"
    },
    "require-dev": {
        "zendframework/zend-serializer": "^2.8",
        "zendframework/zend-log": "^2.9",
        "zendframework/zend-i18n": "^2.7",
        "zendframework/zend-config": "^3.1",
        "phpunit/phpunit": "^5.7 || ^6.0",
        "satooshi/php-coveralls": "^1.0",
        "squizlabs/php_codesniffer": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "Confidences\\ZendGeoip\\": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "Confidences\\ZendGeoip\\Composer\\ScriptHandler::downloadData"
        ],
        "post-update-cmd": [
            "Confidences\\ZendGeoip\\Composer\\ScriptHandler::downloadData"
        ]
    }
}
